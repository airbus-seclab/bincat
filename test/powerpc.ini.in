# used as a config file template for tests

[analyzer]
ini_version = 4
unroll = 150
loglevel = 10
store_marshalled_cfa = true
in_marshalled_cfa_file = "incfa.marshal"
out_marshalled_cfa_file = "outcfa.marshal"
analysis = forward_binary
analysis_ep = 0x00
cut = {code_length}
headers = "libc.no"
{analyzer_section}

[program]
mode = protected
call_conv = svr
mem_sz = 32
op_sz = 32
stack_width = 32
architecture = powerpc
filepath = "{filepath}"
format = raw
{program_section}

[powerpc]
endianness = big
processor_version = 0x200008d0

[state]
reg[cr] = 0x0?0xffffffff
reg[so] = 0?1
reg[ov] = 0?1
reg[ca] = 0?1
reg[tbc] = 0?0x7f
reg[r0] = 0
reg[r1] = 0x1000
reg[r2] = 0
reg[r3] = 0
reg[r4] = 0
reg[r5] = 0
reg[r6] = 0
reg[r7] = 0
reg[r8] = 0
reg[r9] = 0
reg[r10] = 0
reg[r11] = 0
reg[r12] = 0
reg[r13] = 0
reg[r14] = 0
reg[r15] = 0
reg[r16] = 0
reg[r17] = 0
reg[r18] = 0
reg[r19] = 0
reg[r20] = 0
reg[r21] = 0
reg[r22] = 0
reg[r23] = 0
reg[r24] = 0
reg[r25] = 0
reg[r26] = 0
reg[r27] = 0
reg[r28] = 0
reg[r29] = 0
reg[r30] = 0
reg[r31] = 0
reg[lr] = 0
reg[ctr] = 0

#here addresses are supposed to be absolute and not of the form ds:o
mem[0x1000*8192] = |00|?0xFF

{regmem}

[override]
{overrides}
 

[libc]
#tainting rules for libc
#default calling convention for this lib
# call_conv = fastcall
* = open(@, _)
#read uses a different calling convention
* = read<aapcs>(@, *, @)

